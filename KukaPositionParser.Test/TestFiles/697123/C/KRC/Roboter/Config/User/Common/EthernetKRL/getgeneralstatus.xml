<ETHERNETKRL>
	<CONFIGURATION>
		<EXTERNAL>
			<IP>172.20.5.71</IP>
			<PORT>59152</PORT>
		</EXTERNAL>
		<INTERNAL>
			<ENVIRONMENT>Submit</ENVIRONMENT>
			<BUFFERING Mode="FIFO" Limit="16"/>
			<TIMEOUT Connect="5000"/>
			<ALIVE Set_Flag="50"/>
			<PROTOCOL>TCP</PROTOCOL>
			<MESSAGES Logging="error" Display="disabled"/>
		</INTERNAL>
	</CONFIGURATION>
	<RECEIVE>
