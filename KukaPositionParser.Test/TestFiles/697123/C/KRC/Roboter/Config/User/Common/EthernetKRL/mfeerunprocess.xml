<ETHERNETKRL>
	<CONFIGURATION>
		<EXTERNAL>
			<IP>172.20.5.71</IP>
			<PORT>59152</PORT>
		</EXTERNAL>
		<INTERNAL>
			<ENVIRONMENT>Program</ENVIRONMENT>
			<BUFFERING Mode="FIFO" Limit="16"/>
			<TIMEOUT Connect="5000"/>
			<ALIVE Set_Flag="1"/>
			<PROTOCOL>TCP</PROTOCOL>
			<!--<MESSAGES Logging="warning" Display="warning"/> -->
			<MESSAGES Logging="warning" Display="error"/>
		</INTERNAL>
	</CONFIGURATION>
	<RECEIVE>
